
<ul>
<% if @videos %> 
  <%@videos.each do |video|%>
    <li><%=link_to video.title, video.url%> | <%=link_to 'details', video%>
    <%if current_user == video.author%>
      | <%=link_to 'edit', '/videos/'+video.id.to_s+'/edit'%> | <%=link_to 'delete', '/videos'+video.id.to_s, method: :delete%></li>
      <%= link_to 'Upload new video', :method => 'videos/new' %>
      </li>
    <%end%>
  <%end%>
<%end%>
</ul>
